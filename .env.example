# Environment variables for Bot or Not game
# Copy this file to .env and add your actual values

# OpenAI API Key (required)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Set to production for deployment
ENVIRONMENT=development

# Optional: Custom port (default: 8000)
PORT=8000

# Optional: Host binding (default: 0.0.0.0)
HOST=0.0.0.0
